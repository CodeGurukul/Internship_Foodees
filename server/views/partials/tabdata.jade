.tab-content
	#rice.tab-pane.active(role='tabpanel')
		.container
			.row
				.col-md-6.text-center
					each val in menus
						.row
							.col-md-6.jumbotron
								p= val.name
								p= val.featured
								p= val.published
								form(action="/deleteitem/"+val._id method="post")
									button.btn.btn-default(type='submit') Delete        

